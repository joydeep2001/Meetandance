{
	"name": "meetandance",
	"version": "1.0.0",
	"manifest_version": 2,
	"permissions": ["tabs"],
	"content_scripts": [
		{
			"matches": ["https://meet.google.com/*"],
			"js": ["utils.js", "updators.js", "content.js"]
		}
	],
	"browser_action": {
		"default_popup": "popup.html",
		"default_title": "Meetandance"
	}
}
